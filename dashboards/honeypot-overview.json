    {
      "id": 1,
      "title": "Attack Attempts Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 4 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (job) (count_over_time({job=~\"cowrie|opencanary\"} [5m]))",
          "legendFormat": "{{job}}",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "drawStyle": "line",
            "fillOpacity": 25,
            "lineWidth": 2,
            "pointSize": 5,
            "spanNulls": true,
            "stacking": { "mode": "none" }
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "id": 2,
      "title": "Top Attacker IPs",
      "type": "table",
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "topk(20, sum by (src_ip) (count_over_time({job=\"cowrie\"} | json | src_ip != \"\" | __error__=\"\" [$__range])))",
          "queryType": "instant",
          "refId": "A"
        },
        {
          "expr": "topk(20, sum by (src_ip) (count_over_time({job=\"opencanary\"} | json | label_format src_ip=src_host | src_ip != \"\" | __error__=\"\" [$__range])))",
          "queryType": "instant",
          "refId": "B"
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "sortBy",
          "options": { "fields": {}, "sort": [{ "field": "Value", "desc": true }] }
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "src_ip" },
            "properties": [{ "id": "displayName", "value": "Attacker IP" }]
          },
          {
            "matcher": { "id": "byName", "options": "Value" },
            "properties": [{ "id": "displayName", "value": "Attempts" }]
          }
        ]
      }
    },
    {
      "id": 3,
      "title": "Top Targeted Ports",
      "type": "barchart",
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "topk(10, sum by (dst_port) (count_over_time({job=~\"cowrie|opencanary\"} | json | label_format dst_port=`{{ if eq .dst_port \"2222\" }}22{{ else if eq .dst_port \"2223\" }}23{{ else }}{{ .dst_port }}{{ end }}` | dst_port != \"\" | __error__=\"\" [$__range])))",
          "legendFormat": "{{dst_port}}",
          "queryType": "instant",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "fillOpacity": 80,
            "axisBorderShow": false
          }
        },
        "overrides": []
      },
      "options": {
        "orientation": "horizontal",
        "xTickLabelRotation": 0,
        "barWidth": 0.8,
        "groupWidth": 0.7,
        "legend": { "displayMode": "hidden" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 5,
      "title": "Protocol Breakdown",
      "type": "piechart",
      "gridPos": { "h": 10, "w": 10, "x": 0, "y": 22 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (service) (count_over_time({job=~\"cowrie|opencanary\"} | json | label_format dst_port=`{{ if eq .dst_port \"2222\" }}22{{ else if eq .dst_port \"2223\" }}23{{ else }}{{ .dst_port }}{{ end }}` | label_format service=`{{ if eq .dst_port \"22\" }}SSH{{ else if eq .dst_port \"23\" }}Telnet{{ else if eq .dst_port \"25\" }}SMTP{{ else if eq .dst_port \"110\" }}POP3{{ else if eq .dst_port \"5900\" }}VNC{{ else if eq .dst_port \"6379\" }}Redis{{ else if eq .dst_port \"8080\" }}HTTP Proxy{{ else if eq .dst_port \"27017\" }}MongoDB{{ else }}Port {{ .dst_port }}{{ end }}` | service != \"\" | __error__=\"\" [$__range]))",
          "legendFormat": "{{service}}",
          "queryType": "instant",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["percent", "value"]
        },
        "pieType": "donut",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 6,
      "title": "Cowrie Login Attempts",
      "type": "table",
      "gridPos": { "h": 10, "w": 14, "x": 10, "y": 22 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "topk(20, sum by (username, password) (count_over_time({job=\"cowrie\"} | json | eventid=`cowrie.login.failed` | username != \"\" | __error__=\"\" [$__range])))",
          "queryType": "instant",
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "sortBy",
          "options": { "fields": {}, "sort": [{ "field": "Value", "desc": true }] }
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "username" },
            "properties": [{ "id": "displayName", "value": "Username" }]
          },
          {
            "matcher": { "id": "byName", "options": "password" },
            "properties": [{ "id": "displayName", "value": "Password" }]
          },
          {
            "matcher": { "id": "byName", "options": "Value" },
            "properties": [{ "id": "displayName", "value": "Attempts" }]
          }
        ]
      }
    },
    {
      "id": 4,
      "title": "Cowrie Command Log",
      "type": "logs",
      "gridPos": { "h": 12, "w": 12, "x": 0, "y": 32 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "{job=\"cowrie\"} | json | eventid = `cowrie.command.input` | line_format `{{.src_ip}} $ {{.input}}`",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showCommonLabels": false,
        "showLabels": false,
        "sortOrder": "Descending",
        "enableLogDetails": true,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "dedupStrategy": "none"
      }
    },
    {
      "id": 8,
      "title": "Recent Activity",
      "type": "logs",
      "gridPos": { "h": 12, "w": 12, "x": 12, "y": 32 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "{job=~\"cowrie|opencanary\"} | json | __error__=\"\" | line_format `{{.job}} | {{ if .src_ip }}{{.src_ip}}{{ else }}{{.src_host}}{{ end }} \u2192 port {{.dst_port}}`",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showCommonLabels": false,
        "showLabels": true,
        "sortOrder": "Descending",
        "enableLogDetails": true,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "dedupStrategy": "none"
      }
    }
