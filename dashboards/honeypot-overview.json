    {
      "id": 13,
      "title": "Targeted Ports",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "count(sum by (dst_port) (count_over_time({job=~\"cowrie|opencanary\"} | json | label_format dst_port=`{{ if eq .dst_port \"2222\" }}22{{ else if eq .dst_port \"2223\" }}23{{ else }}{{ .dst_port }}{{ end }}` | dst_port != \"\" | __error__=\"\" [$__range])))",
          "queryType": "instant",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "orange", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
